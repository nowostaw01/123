pipeline {
    agent any
    environment [

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Scripting') {
            steps {
                parallel [
                "taskOne" :{
                    echo 'Task one stuff:'
                    echo 'Deploying....'
                },
                "taskTwo" :{
                    echo 'task second stuff:'
                }
            ]
        }
    }
}
]
}
