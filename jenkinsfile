pipeline {
    agent any
    environment {
dotnet = '"c:\\Program Files\\dotnet\\dotnet.exe"'
DEPLOY_TO = "main environment"
}

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Scripting') {
            steps {
                parallel [
                "taskOne" :{
                    echo 'Task one stuff:'
                    echo 'Deploying....'
                },
                "taskTwo" :{
                    echo 'task second stuff:'
                }
            ]
        }
    }
}
}
